# Metabase environment example (populated by startup.sh on the VM)

# Site
MB_SITE_URL=
MB_CREATE_SAMPLE_DATA=false

# Metabase application DB (Postgres)
MB_DB_TYPE=postgres
MB_DB_DBNAME=metabase
MB_DB_USER=metabase_app
MB_DB_PASS=
MB_DB_HOST=metabase-db
MB_DB_PORT=5432

# Encryption
MB_ENCRYPTION_SECRET=

# BigQuery connection (used by Metabase BigQuery driver)
MB_BIGQUERY_PROJECT_ID=
MB_BIGQUERY_DATASET=
MB_BIGQUERY_SERVICE_ACCOUNT=

# Backups
MB_BACKUP_BUCKET=
LOG_LEVEL=INFO
